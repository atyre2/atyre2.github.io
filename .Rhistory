sexed <- utility(1:10, price = 3)
utility <- function(h, price = NULL, maxhens = 4){
if (is.null(price)){
stop("No value for price and no default.")
}
u <- ifelse(h < maxhens+1, h, maxhens)
return(u/(h*price))
}
